{
    "plugins": ["prettier","cypress"],
    "ignorePatterns": ["node_modules/**/*.js"],
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["plugin:prettier/recommended", "eslint:recommended", "plugin:cypress/recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "globals": {
        "__dirname": true,
        "process": true,
        "_": true
    },
    "rules": {
        "prettier/prettier": "error",
        "cypress/no-unnecessary-waiting": 0
    }
}